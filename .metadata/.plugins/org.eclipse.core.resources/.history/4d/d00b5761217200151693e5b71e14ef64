public class ProjectNameExecution extends ProjectNamePanel  {

	public ProjectNameExecution(Window window) 
	{
		super(window);
	}

	//Method that will continually run until we exit the program 
	public void start(Window window) 
	{
		while(true) 
		{ 
			try 
			{ 
				Thread.sleep(10);

				repaint(); 
			}catch(Exception e) 
			{ 
				e.printStackTrace(); 
			} 
		} 

	}



}
