package com.saucypixelstudios.trixeltechnician.UI;

import java.awt.Graphics;

public class MenuElement {
	
	private int width;
	private int height;

	public MenuElement(int width, int height) {
		this.width = width;
		this.height = height;
	}
	
	public int getWidth() {
		return width;
	}
	
	public int getHeight() {
		return height;
	}
	
	public abstract void drawElement(Graphics g);
}
