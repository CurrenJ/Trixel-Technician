package com.saucypixelstudios.trixeltechnician.UI;

public class Element {
	
	private int width;
	private int height;

	public Element(int width, int height) {
		this.width = width;
		this.height = height;
	}
	
	public int getWidth() {
		return width;
	}
	
	public int getHeight() {
		return height;
	}
	
	public abstract void drawElement()
}
